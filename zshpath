# Install Ruby Gems in the user directory
export GEM_HOME="$HOME/.gem"
export PATH="$PATH:$GEM_HOME/bin"

export PATH="$PATH:$HOME/Code/flutter/flutter/bin"
export PATH="$PATH:$HOME/Applications/flutter/bin"

# Google Cloud SDK

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/george/google-cloud-sdk/path.zsh.inc' ]; then
    . '/Users/george/google-cloud-sdk/path.zsh.inc'
fi
# The next line enables shell command completion for gcloud.
if [ -f '/Users/george/google-cloud-sdk/completion.zsh.inc' ]; then
    . '/Users/george/google-cloud-sdk/completion.zsh.inc'
fi

